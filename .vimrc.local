
" Environment {
    " Windows Compatible {
    " On Windows, also use '.vim' instead of 'vimfiles'; this makes synchronization
    " across (heterogeneous) systems easier.
    if has('win32') || has('win64')
        let $RUNTIME=expand("$VIM/bin")
        set runtimepath^=$RUNTIME
        ""set runtimepath+=$VIM . '/bin'
    endif
    " }
" }

